@model MyPortfolio.Models.TblSkills
@{
    ViewBag.Title = "AddSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yetenek Ekleme Sayfası</h2>


<form method="post" id="skillForm">
    <label>Teknoloji Adı</label>
    <input type="text" name="SkillName" class="form-control" />
    <br />
    <label>Başarı Değeri</label>
    <input type="text" name="Value" class="form-control" id="successValue" required />
    <br />
    <button type="submit" class="btn btn-outline-success">Kaydet</button>
</form>

